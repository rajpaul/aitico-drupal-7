#user-cancel-confirm-form #edit-user-cancel-method{
    display: none;
}
#user-cancel-confirm-form .form-item-user-cancel-confirm{
    display: none;
}

#edit-body #edit-body-und-0-format , #edit-body .form-item-body-und-0-summary {
    display:none;
}

#tab-description fieldset.filter-wrapper{
    display:none;
}

#tab-group div:first{
    border: 1px solid #C5D0DC;
    padding: 16px 12px;
    position: relative;
    z-index: 11;
}
#tab-group div:first {
    overflow: auto;
}
#user-delete,#user-edit{
    display:none;
}


.group-wallpaper .form-required {display: none}
.form-managed-file input[value="Upload"] {
    display: none;
}
#user-delete,#user-edit{
    display:none;
}
#edit-field-next-update{
    display: none;
}

.group-wallpaper .span2 {margin-left: 10px !important}

.loader_right {
    width:20%; 
    float: right; 
    padding-top: 15px; 
    padding-right: 50px; 
    text-align: right;
}
.title_left{
    width:50%; 
    float: left;
}

#schedule-table .span2 {
    width:100% !important;
}

#schedule-table span.form-required {
 display: none;   
}

#schedule-table img.text_thumblain {width:140px; height: 140px;}

.clear-right-block{
    clear:both;
    height: 1px;
}
#search_logs,#filegroup-add-group{
    height: 30px;
}
.btn-cancel{
    height: 35px;
    margin-left: 10px !important;
    font-size: 14px !important;
    line-height: 35px !important;
}

.pull-left {
    margin-bottom: 12px;
}
.pull-right{
    margin-bottom: 12px;
}
.hidden-link{
    display:none;
}
@-moz-document url-prefix()
{ 
   .btn-cancel{
    height: 21px;
    line-height: 18px !important;
}
}