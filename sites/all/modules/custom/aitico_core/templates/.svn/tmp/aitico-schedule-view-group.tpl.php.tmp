<thead>
    <tr>
        <th class="span4"><?php print t('Date of the update'); ?></th>
        <?php for ($i = 1; $i <= $slot_number; $i++): ?>
            <th><?php print t('Slot ') . $i; ?></th>
        <?php endfor; ?>
    </tr>
</thead>
<tbody>
    <?php foreach ($schedule_info as $key => $val): ?>
